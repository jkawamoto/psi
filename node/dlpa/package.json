{
  "name": "node-red-contrib-psi-dlpa",
  "version": "0.3.2",
  "description": "Distributed LPA node for Psi project",
  "author": "Junpei Kawamoto <junpei.kawamoto@acm.org> (https://www.jkawamoto.info/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkawamoto/psi.git"
  },
  "keywords": [
    "node-red"
  ],
  "node-red": {
    "nodes": {
      "dlpa": "lib/dlpa.js"
    }
  },
  "scripts": {
    "test": "python setup.py test",
    "deps": "pip install -r requirements.txt",
    "build": "gulp build",
    "gulp": "gulp",
    "docs": "cd docs && make html",
    "pandoc": "pandoc --from markdown --to rst --out README.rst README.md",
    "check-update": "ncu -u"
  },
  "files": [
    "lib",
    "src",
    "gulpfile.js",
    "COPYING",
    "package.json",
    "README.md"
  ],
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-exec": "^2.1.3",
    "gulp-htmlmin": "^3.0.0",
    "gulp-pug": "^3.2.0",
    "gulp-uglify": "^2.0.1",
    "jstransformer-babel": "^2.1.0",
    "jstransformer-markdown-it": "^2.0.0",
    "npm-check-updates": "^2.10.2"
  },
  "private": true,
  "dependencies": {
    "grpc": "^1.1.2"
  }
}
